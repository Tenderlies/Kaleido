Windows Registry Editor Version 5.00

[HKEY_CLASSES_ROOT\Directory\Background\shell\cygwin_bash]
@="Cygwin Bash Here"
"Icon"="D:\\env\\cygwin\\bin\\mintty.exe"

[HKEY_CLASSES_ROOT\Directory\Background\shell\cygwin_bash\command]
@="D:\\env\\cygwin\\bin\\mintty.exe -i D:\\env\\cygwin\\Cygwin.ico D:\\env\\cygwin\\bin\\bash.exe --login -c \"cd '%V'; exec bash -rcfile ~/.bashrc\""